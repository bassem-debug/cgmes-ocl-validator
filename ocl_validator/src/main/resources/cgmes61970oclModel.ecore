              <details key="VoltageTargetsAtTN" value="&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;--docReference QoCDCv3 Level=6 ruleID=VoltageTargetsAtTN&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;let trmnl = self.Terminal-&gt;select(RegulatingControl-&gt;notEmpty()) in&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;let rcc = trmnl.RegulatingControl-&gt;select(enabled and not(discrete)) in&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;let first_rcc = rcc-&gt;asSequence()-&gt;at(1) in&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;if self.DataSet.validationScope = ValidationScope::QOCDCV3_1 &#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;and self.DataSet.global_level_validation&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;--and not(self.DataSet.excludeProvedRules)&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;and rcc-&gt;notEmpty() &#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;and rcc-&gt;size() &gt;= 2 &#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;then rcc-&gt;forAll(first_rcc.targetValue = targetValue)&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;else true endif"/>
              <details key="VoltageTargetAndDeadbandAtTN" value="&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;--docReference QoCDCv3 Level=6 ruleID=VoltageTargetAndDeadbandAtTN&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;let trmnl = self.Terminal-&gt;select(RegulatingControl-&gt;notEmpty()) in&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;let drcc = trmnl.RegulatingControl-&gt;select(discrete and enabled) in&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;let arcc = trmnl.RegulatingControl-&gt;select(not(discrete) and enabled) in&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;let max_db = drcc.targetDeadband-&gt;max() in&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;let max_drcc = drcc-&gt;select(targetDeadband = max_db)-&gt;asSequence()-&gt;at(1) in&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;if self.DataSet.validationScope = ValidationScope::QOCDCV3_1 &#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;and self.DataSet.global_level_validation&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;--and not(self.DataSet.excludeProvedRules)&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;and drcc-&gt;size() &gt;= 2 &#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;and drcc-&gt;forAll(targetDeadband-&gt;notEmpty())&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;then drcc-&gt;forAll((max_drcc.targetValue + max_drcc.targetDeadband/2 &gt;= targetValue + targetDeadband/2&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;and max_drcc.targetValue - max_drcc.targetDeadband/2 &lt; targetValue + targetDeadband/2)&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;or&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;(max_drcc.targetValue + max_drcc.targetDeadband/2 &gt; targetValue - targetDeadband/2&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;and max_drcc.targetValue - max_drcc.targetDeadband/2 &lt;= targetValue - targetDeadband/2)&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;)&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;and&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;arcc-&gt;forAll((max_drcc.targetValue + max_drcc.targetDeadband/2 &gt;= targetValue)&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;and max_drcc.targetValue - max_drcc.targetDeadband/2 &lt;= targetValue)&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;else true endif&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;and&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;if self.DataSet.validationScope = ValidationScope::QOCDCV3_1 &#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;and self.DataSet.local_level_validation&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;--and not(self.DataSet.excludeProvedRules)&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;and drcc-&gt;size() = 1 &#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;and drcc-&gt;forAll(targetDeadband-&gt;notEmpty())&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;and arcc-&gt;size() &gt; 0&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;then arcc-&gt;forAll(max_drcc.targetValue + max_drcc.targetDeadband/2 &gt;= targetValue&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;and max_drcc.targetValue - max_drcc.targetDeadband/2 &lt;= targetValue)&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;else true endif"/>
              <details key="EnergizedBoundaryTN" value="&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;--docReference QoCDCv3 Level=6 ruleID=EnergizedBoundaryTN&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;let svv = self.SvVoltage in&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;let trmnls = self.Terminal in&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;let eib = trmnls.ConductingEquipment-&gt;select(oclIsKindOf(Equivalents::EquivalentInjection)) in&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;let ei = eib-&gt;asSequence()-&gt;at(1).oclAsType(Equivalents::EquivalentInjection) in&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;if self.DataSet.validationScope = ValidationScope::QOCDCV3_1 &#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;and self.DataSet.global_level_validation&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;--and not(self.DataSet.excludeProvedRules)&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;and svv-&gt;notEmpty()&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;and trmnls-&gt;notEmpty()&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;and eib-&gt;notEmpty()&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;and ei-&gt;notEmpty()&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;and self.boundaryPoint&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;then  ei.p-&gt;notEmpty() and ei.q-&gt;notEmpty() and svv.v-&gt;notEmpty() and svv.v &gt; 0&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;else true endif"/>
              <details key="ACTielineBV" value="&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;--docReference QoCDCv3 Level=7 ruleID=ACTielineBV&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;let trmnl = self.Terminal in&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;let aclines = trmnl.ConductingEquipment-&gt;selectByType(Wires::ACLineSegment)-&gt;asSequence() in&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;let acline1 = aclines-&gt;at(1) in&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;let acline2 = aclines-&gt;at(2) in&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;let bv1 = acline1.BaseVoltage in&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;let bv2 = acline2.BaseVoltage in&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;let bvdiff = bv1.nominalVoltage - bv2.nominalVoltage in&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;if self.DataSet.validationScope = ValidationScope::QOCDCV3_1 &#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;and self.DataSet.global_level_validation&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;--and not(self.DataSet.excludeProvedRules)&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;and self.DataSet.type = DataSetType::cgm&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;and self.boundaryPoint &#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;and (self.description = null or self.description.size() &lt; 4 or self.description.substring(1,4) &lt;&gt; 'HVDC')&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;and trmnl-&gt;notEmpty()&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;and aclines-&gt;size() = 2&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;and bv1-&gt;notEmpty() and bv2-&gt;notEmpty()&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;then bvdiff.abs()/bv1.nominalVoltage &lt; 0.1  else true endif"/>
              <details key="UnpairedTieFlow" value="&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;--docReference QoCDCv3 Level=7 ruleID=UnpairedTieFlow&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;let trmnls = self.Terminal in&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;let eis = trmnls.ConductingEquipment-&gt;select(oclIsTypeOf(Equivalents::EquivalentInjection))-&gt;asSequence() in&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;let ei1 = eis-&gt;at(1).oclAsType(Equivalents::EquivalentInjection) in&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;let ei2 = eis-&gt;at(2).oclAsType(Equivalents::EquivalentInjection) in&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;if self.DataSet.validationScope = ValidationScope::QOCDCV3_1 &#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;and self.DataSet.global_level_validation&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;--and not(self.DataSet.excludeProvedRules)&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;and self.boundaryPoint = true&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;and (self.description = null or self.description.size() &lt; 4 or self.description.substring(1, 4) &lt;&gt; 'HVDC')&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;and trmnls-&gt;notEmpty()&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;and eis-&gt;size() = 2&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;and ei1.p-&gt;notEmpty()&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;and ei2.p-&gt;notEmpty()&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;and ei1.q-&gt;notEmpty()&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;and ei2.q-&gt;notEmpty()&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;then (ei1.p = 0 and ei2.p = 0 and ei1.q = 0 and ei2.q = 0)&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;or&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;(ei1.p &lt;&gt; 0 and ei2.p &lt;&gt; 0 and ei1.q &lt;&gt; 0 and ei2.q &lt;&gt; 0)&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;else true endif"/>
              <details key="RequiredSvVoltage" value="&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;--docReference QoCDCv3 Level=6 ruleID=RequiredSvVoltage&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;let svv = self.SvVoltage in&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;if self.DataSet.validationScope = ValidationScope::QOCDCV3_1 &#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;and self.DataSet.global_level_validation&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;--and not(self.DataSet.excludeProvedRules)&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;and self.boundaryPoint = false&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;then svv-&gt;notEmpty()&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;else true endif"/>
              <details key="InconsistentCurrentLimits" value="&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;--docReference QoCDCv3 Level=7 ruleID=InconsistentCurrentLimits&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;let tn = self in&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;let trmnls = self.Terminal in&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;let aclines = trmnls.ConductingEquipment-&gt;selectByType(Wires::ACLineSegment)-&gt;asSequence() in&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;let acline1 = aclines-&gt;at(1) in&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;let acline2 = aclines-&gt;at(2) in&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;let trmnl1 = acline1.Terminals-&gt;select(TopologicalNode = tn)-&gt;asSequence()-&gt;at(1) in&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;let trmnl2 = acline2.Terminals-&gt;select(TopologicalNode = tn)-&gt;asSequence()-&gt;at(1) in&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;let cls1 = trmnl1.OperationalLimitSet.OperationalLimitValue-&gt;selectByType(OperationalLimits::CurrentLimit) in&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;let cls2 = trmnl2.OperationalLimitSet.OperationalLimitValue-&gt;selectByType(OperationalLimits::CurrentLimit) in&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;let cls1patl = cls1-&gt;select(OperationalLimitType.limitType = OperationalLimits::LimitTypeKind::patl) in&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;let cls2patl = cls2-&gt;select(OperationalLimitType.limitType = OperationalLimits::LimitTypeKind::patl) in&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;let cl1patl = cls1patl-&gt;asSequence()-&gt;at(1) in&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;let cl2patl = cls2patl-&gt;asSequence()-&gt;at(1) in&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;if self.DataSet.validationScope = ValidationScope::QOCDCV3_1 &#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;and self.DataSet.global_level_validation&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;--and not(self.DataSet.excludeProvedRules)&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;and self.DataSet.type = DataSetType::cgm&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;and self.boundaryPoint&#xA;&#x9;&#x9;&#x9;&#x9;&#x9