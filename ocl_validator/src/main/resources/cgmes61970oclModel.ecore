              <details key="SmallTopologicalIsland" value="&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;--docReference QoCDCv3 Level=5 ruleID=SmallTopologicalIsland&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;let tns = self.TopologicalNodes in&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;if self.DataSet.validationScope = ValidationScope::QOCDCV3_1 &#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;and self.DataSet.global_level_validation&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;--and not(self.DataSet.excludeProvedRules)&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;then  tns-&gt;size() &gt; 3 or tns-&gt;forAll(SvVoltage-&gt;notEmpty() and SvVoltage.v-&gt;notEmpty() and SvVoltage.v &lt;&gt; 0) else true endif"/>
              <details key="SlackNode" value="&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;--docReference QoCDCv3 Level=5 ruleID=SlackNode&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;let artn = self.AngleRefTopologicalNode in&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;let ar_trmnl = artn.Terminal in&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;let smp = ar_trmnl.ConductingEquipment-&gt;select(oclIsTypeOf(Wires::SynchronousMachine)).oclAsType(Wires::SynchronousMachine) in&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;let all_tns = self.TopologicalNodes in&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;let trmnls = all_tns.Terminal in&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;let sms = trmnls.ConductingEquipment-&gt;select(oclIsTypeOf(Wires::SynchronousMachine)).oclAsType(Wires::SynchronousMachine) in&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;let rest_sms = sms-&gt;select(s | smp-&gt;excludes(s)) in&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;if self.DataSet.validationScope = ValidationScope::QOCDCV3_1 &#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;and self.DataSet.global_level_validation&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;--and not(self.DataSet.excludeProvedRules)&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;and artn-&gt;notEmpty()&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;and ar_trmnl-&gt;notEmpty()&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;and all_tns-&gt;size() &gt; 1&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;and sms-&gt;size() &gt; 0&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;and smp-&gt;size() &gt; 0&#xA;&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;then smp-&gt;exists(referencePriority &gt; 0)&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;and (smp.referencePriority-&gt;max() &lt;= rest_sms.referencePriority-&gt;max()&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;or rest_sms.referencePriority-&gt;max() = 0)&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;else true endif"/>
